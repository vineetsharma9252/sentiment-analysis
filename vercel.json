
{

    "builds": [
        {
            "src": "test_api_enhanced.py",
            "use": "@vercel/python",
            "config": { "maxLambdaSize": "15mb" }
        }
    ],
    "routes": [
        { "src": "/(.*)", "dest": "test_api_enhanced.py" }
    ] , 
    "env": {
        "MODEL_PATH": "./models/model.pkl"
    }
}